{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "framework": "vite"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/.*",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}